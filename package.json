{
	"name": "my-ink-cli",
	"version": "0.0.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"type": "module",
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"build": "bun build  --format=esm --target bun source/cli.tsx --outdir dist",
		"build-binary": "bun build --compile --bytecode --format=esm --target bun source/cli.tsx --outfile mycli"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"ink": "^6",
		"meow": "^14.0.0",
		"react": "^19",
		"react-devtools-core": "^7.0.1"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^8.1.0",
		"@types/react": "^19",
		"chalk": "^5.2.0",
		"prettier": "^3.8.1",
		"ts-node": "^10.9.1",
		"tsdown": "^0.20.1",
		"typescript": "^5.0.3"
	}
}
